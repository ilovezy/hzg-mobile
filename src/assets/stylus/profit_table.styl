.profit_table-nav
  position: fixed
  width 100%
  height .88rem
  top 0px
.profit_table-navleft
  text-align right
  color #222222
  font-size .34rem
  line-height .88rem
  width 60%
  float left
.profit_table-navright
  text-align right
  color #666666
  font-size .22rem
  line-height .88rem
  float right
  width 40%
  color #bbbbbb
.profit_table-list
  background-color #d62d2d
  height 1rem
  font-size 12px
  width: 100%
  color #FFFFFF
  ul
   overflow hidden
   height 100%
   width 100%
   li
    height 100%
  li:nth-of-type(1)
     width 10%
     float left
     line-height 1rem
     text-align center
  li:nth-of-type(2)
     width 20%
     float left
     line-height 1rem
     text-align center
  li:nth-of-type(3)
     float left
     line-height 1rem
     text-align center
     width 50%
  li:nth-of-type(4)
     float left
     line-height .5rem
     width 20%
     text-align center
.profit-table-list
  background-color #FFFFFF
.profit-table-item
  height 1.56rem
  ul
    overflow hidden
    height 100%
    width 100%
    li
      height 100%
      >p
        margin 0
        font-size 14px
      >span
        font-size 10px
        color #666666
  li:nth-of-type(1)
    width 10%
    box-sizing border-box
    float left
    text-align center
  li:nth-of-type(2)
    width 35%
    float left
    line-height .5rem
    text-align left
    padding-top .2rem
    border-bottom 0.5px solid #eeeeee
  li:nth-of-type(3)
    width 30%
    float left
    line-height .5rem
    text-align left
    padding-top .2rem
    border-bottom 0.5px solid #eeeeee

  li:nth-of-type(4)
    width 25%
    line-height .5rem
    float left
    text-align right
    padding-right 3%
    padding-top .2rem
    border-bottom 0.5px solid #eeeeee
.fail-bar
  height 1.56rem
  width .04rem
  margin 0 auto
  background-color #EEEEEE
  position relative
  >div
    width .2rem
    height .2rem
    border-radius 50%
    background-color #eeeeee
    position absolute
    left -.08rem
    top .75rem
.success
  position relative
  height 100%
.success-bar
  height .53rem
  width .04rem
  background-color #d82d2d
  position: absolute
  top 0rem
  left calc(50% - 0.02rem)
.icon-checkbox
  color #d82d2d
  font-size 20px
  position absolute
  left .15rem
  top .5rem
.success-footer
  height .53rem
  width .04rem
  background-color #d82d2d
  position: absolute
  bottom 0rem
  left calc(50% - 0.02rem)





